{
  "expo": {
    "name": "ReactNativeAppTemplate",
    "slug": "ReactNativeAppTemplate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/expoAssets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/expoAssets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/expoAssets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/expoAssets/favicon.png"
    }
  }
}
